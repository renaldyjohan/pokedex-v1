{
  "extends": "next/core-web-vitals",
  "rules": {
    "@next/next/no-img-element": "off",
    "indent": [
      "error",
      2
    ],
    "no-console": [
      0
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ]
  }
}
